{
  "name": "github-wrapped",
  "version": "1.0.0",
  "description": "GitHub Wrapped - Visualize your GitHub activity for the year",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "worker:dev": "wrangler dev worker/index.js",
    "worker:deploy": "wrangler deploy --define BUILD_TIMESTAMP:$(date +%s)",
    "dev:local": "bun run build && wrangler dev",
    "deploy": "bun run build && wrangler deploy --define BUILD_TIMESTAMP:$(date +%s)"
  },
  "keywords": ["github", "wrapped", "vue", "cloudflare-workers"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "isomorphic-git": "^1.27.1",
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.3",
    "vite": "^7.3.0",
    "wrangler": "^4.54.0"
  }
}
